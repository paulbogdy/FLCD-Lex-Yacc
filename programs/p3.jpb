func master() {
    mut n: int
    read(n)
    
    elems: elems[1000]

    mut nr: int = 0

    while(n > 1) {
        elems[nr] = n
        if(n % 2 == 1) {
            n = n*3 + 1
        }
        else {
            n = n/2
        }
        nr = nr+1
    }
    for(mut i: int = 0, i < n, i = i+1) {
        write(elems[i])
    }
}
